@model IEnumerable<StudentManagement.Models.Student>
<h1>Student: Erick Ovando Perez</h1>
<h2>Student ID: 200579494</h2>
<h2>Students List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Age</td>
                <td>@student.Course</td>
            </tr>
        }
    </tbody>
</table>